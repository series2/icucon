GET     /initialize                 controllers.IsuController.initialize()
GET     /login                      controllers.IsuController.showLogin()
POST    /login                      controllers.IsuController.newLogin()
GET     /register                   controllers.IsuController.showRegister()
POST    /register                   controllers.IsuController.register()
GET     /logout                     controllers.IsuController.logout()
GET     /                           controllers.IsuController.index()
GET     /@:accountName              controllers.IsuController.showAccount(accountName: String)
GET     /posts                      controllers.IsuController.posts()
GET     /posts/:id                  controllers.IsuController.showPost(id: Int)
POST    /                           controllers.IsuController.createPost()
GET     /image/:id.:ext             controllers.IsuController.showImage(id: Int, ext: String)
POST    /comment                    controllers.IsuController.createComment()
GET     /admin/banned               controllers.IsuController.banned()
POST    /admin/banned               controllers.IsuController.ban()

